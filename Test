local Window = MakeWindow({
    Hub = {
        Title = "Tổng Hợp GAG"
    },
    Key = {
        KeySystem = false,
        Title = "Key System",
        Description = "",
        KeyLink = "",
        Keys = {"1234"},
        Notifi = {
            Notifications = true,
            CorrectKey = "Running the Script...",
            Incorrectkey = "The key is incorrect",
            CopyKeyLink = "Copied to Clipboard"
        }
    }
})

MinimizeButton({
    Image = "https://i.postimg.cc/4yKy6DsH/t-i-xu-ng-5.jpg",
    Size = {60, 60},
    Color = Color3.fromRGB(10, 10, 10),
    Corner = true,
    Stroke = false,
    StrokeColor = Color3.fromRGB(255, 0, 0)
})

-- Tab Grow A Garden
local Tab1o = MakeTab({Name = "Script Grow a garden"})

local function AddGAGScript(name, url)
    AddButton(Tab1o, {
        Name = name,
        Callback = function()
            loadstring(game:HttpGet(url))()
        end
    })
end

-- Danh sách Script Grow A Garden đầy đủ:
AddGAGScript("No Lag GAG", "https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/refs/heads/main/Loader/LoaderV2.lua")
